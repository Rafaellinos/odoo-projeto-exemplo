<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="covid_sintomas_action" model="ir.actions.act_window">
        <field name="name">Sintomas</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">covid.sintomas</field>
        <field name="view_mode">tree,form</field>
    </record>


    <record id="covid_sintomas_form" model="ir.ui.view">
        <field name="name">covid.sintomas.view.form</field>
        <field name="model">covid.sintomas</field>
        <field name="arch" type="xml">
            <form>
                <header>
                </header>
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="covid_sintomas_view_tree" model="ir.ui.view">
        <field name="name">covid.sintomas.view.tree</field>
        <field name="model">covid.sintomas</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

</odoo>